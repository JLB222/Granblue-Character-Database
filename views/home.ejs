<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Granblue Character Database</title>
    <link rel="stylesheet" type="text/css" href="css/styles.css">
  </head>
  <body>
    <h1>Granblue Character Database</h1>
    <ul>
    <%  characterArray.forEach(function(character) { %>
        <li><span class="<%= character.Element %> element"></span><%= character.Name %>%></li>
    <%  }); %>
    </ul>
  </body>
</html>
